{
  "manifest_version": 3,
  "name": "Top 10 History",
  "version": "1.0.0",
  "description": "Track browsing history by host with latest 10 URLs per host",
  "permissions": ["history","storage","activeTab","scripting","tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Web History Summary"
  },
  "host_permissions": ["<all_urls>"]
}
